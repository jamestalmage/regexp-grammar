  "author": {
    "name": "James Talmage",
    "email": "james@talmage.io",
    "url": "github.com/jamestalmage"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "npm run cover && npm run lint && npm run style",
    "cover": "istanbul cover ./node_modules/.bin/_mocha -- --harmony",
    "lint": "jshint *.js lib/*.js lib/**/*.js test/*.js test/**/*.js",
    "debug": "mocha --harmony",
    "watch": "mocha -w --harmony --watch-extensions pegjs",
    "build": "node build.js",
    "style": "jscs *.js ./**/*.js && jscs ./test/** --config=./test/.jscsrc"
  },
  "files": [
    "index.js"
  ],
  "keywords": [
    "regexp",
    "parser"
  ],
  "dependencies": {},
  "devDependencies": {
    "codeclimate-test-reporter": "^0.1.0",
    "coveralls": "^2.11.2",
    "istanbul": "^0.3.17",
    "jscs": "^1.13.1",
    "jshint": "^2.8.0",
    "mocha": "^2.2.5",
    "pegjs": "git@github.com:pegjs/pegjs.git",
    "regenerate": "^1.2.1",
    "sinon": "^1.15.4",
    "unicode-3.0.1": "^0.1.5"
  }
}
